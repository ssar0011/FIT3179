{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 800,
  "height": 500,
  "title": "Passenger Load Factor in 2019",
  "projection": {"type": "equirectangular"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ssar0011/FIT3179/main/HW9/js/STE_2016_AUST.topojson",
        "format": {"type": "topojson", "feature": "STE_2016_AUST"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ssar0011/FIT3179/main/HW9/data/audomcitypairs-201912.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "Dep_Long", "type": "quantitative"},
        "latitude": {"field": "Dep_Lat", "type": "quantitative"},
        "size": {
          "field": "Passenger_Load_Factor",
          "type": "quantitative"
        }
      }
    }
  ]
}
