{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "COVID-19 Active Cases (10 Oct 2020)",
  "width": 800,
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/ssar0011/FIT3179/main/chloropleth_map/STE_2016_AUST.topojson",
    "format": {"type": "topojson", "feature": "STE_2016_AUST"}
  },
  "transform": [
    {
      "lookup": "properties.STE_NAME16",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/ssar0011/FIT3179/main/chloropleth_map/data"
        },
        "key": "Jurisdiction",
        "fields": ["Fire_area"]
      }
    }
  ],
  "projection": {"type": "equirectangular"},
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Fire_area",
      "type": "quantitative"
    },
    "tooltip": [
      {"field": "properties.STE_NAME16", "type": "nominal", "title": "State"},
      {"field": "Fire_area", "type": "quantitative"}
    ]
  }
}
